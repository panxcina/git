    <form name="viewProductUpdate" type="list" list-name="bulkProductUpdates" use-row-submit="true" target="updateMultipleProducts"
        header-row-style="header-row-2" default-table-style="basic-table hover-bar" odd-row-style="alternate-row" view-size="300" paginate-target="viewProductUpdate">
        <field name="fileName"><hidden value="${fileName}"/></field>
        <field name="updateProduct" title=" " widget-style="buttontext" position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productUpdateId&quot;);
            if (prev == null) prev = &quot;allowFirst&quot;;
            return new Boolean(prev!=null&amp;&amp;!(previousItem.get(&quot;productUpdateId&quot;).equals(productUpdateId))&amp;&amp;updatedStatus.equals(&quot;N&quot;));}">
            <hyperlink target="updateProductIntoBulkProductUpdate" description="Update" also-hidden="false">
                <parameter param-name="updateFormId" from-field="updateFormId"/>
                <parameter param-name="fileName" from-field="fileName"></parameter>
                <parameter param-name="productUpdateId" from-field="productUpdateId"></parameter>
            </hyperlink>
        </field>
        <field name="editProduct" title=" " widget-style="buttontext" position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productUpdateId&quot;);
