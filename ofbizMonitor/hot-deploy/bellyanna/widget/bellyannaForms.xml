                <parameter param-name="fileName" from-field="path"/>
                <parameter param-name="name"/>
            </hyperlink>
        </field>
        <field name="deleteFileProductImport" title="" widget-style="buttontext">
            <hyperlink target="deleteFileProductImport" description="Clear Temporary Data and File" also-hidden="false">
                <parameter param-name="importFormId"/>
                <parameter param-name="fileName" from-field="path"/>
