    <form name="viewProductImport" type="list" list-name="productImports" use-row-submit="true" target="importMultipleProducts"
        header-row-style="header-row-2" default-table-style="basic-table hover-bar" odd-row-style="alternate-row" view-size="300" paginate-target="viewProductImport">
        <field name="fileName"><hidden value="${fileName}"/></field>
        <field name="createProduct" title=" " widget-style="buttontext" position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);
            if (prev == null) prev = &quot;allowFirst&quot;;
            return new Boolean(prev!=null&amp;&amp;!(previousItem.get(&quot;productImportId&quot;).equals(productImportId))&amp;&amp;importedStatus.equals(&quot;N&quot;));}">
            <hyperlink target="importNewProductForImportAndMessage" description="Import" also-hidden="false">
                <parameter param-name="importFormId" from-field="importFormId"/>
                <parameter param-name="fileName" from-field="fileName"></parameter>
                <parameter param-name="productImportId" from-field="productImportId"></parameter>
            </hyperlink>
        </field>
        <field name="editProduct" title=" " widget-style="buttontext" position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);
            if (prev == null) prev = &quot;allowFirst&quot;;
            return new Boolean(prev!=null&amp;&amp;!(prev.equals(productImportId))&amp;&amp;importedStatus.equals(&quot;N&quot;));}">
            <hyperlink target="editTemporaryProduct" description="Edit" also-hidden="false" link-type="hidden-form">
                <parameter param-name="productImportId"/>
                <parameter param-name="importFormId"/>
                <parameter param-name="fileName"/>
                <parameter param-name="imported"/>
            </hyperlink>
        </field>
        <!-- ProductImport entity fields -->
        <field name="productImportId" title="Product Import Id" position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="fileLineNumber" title="File Line Number" position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="productId" title="Product Id"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId))&amp;&amp;importedStatus.equals(&quot;N&quot;));}"><display/></field>
        <field name="productId" title="Product Id"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId))&amp;&amp;importedStatus.equals(&quot;Y&quot;));}" widget-style="buttontext">
            <hyperlink target="/catalog/control/EditProduct" description="${productId}" target-type="inter-app" target-window="blank">
                <parameter param-name="productId"/>
            </hyperlink>
        </field>
        <field name="internalName" title="Product Internal Name"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="productType" title="Product Type"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="facilityId" title="Warehouse" position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="locationCode" title="Location Code"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="weight" title="Weight"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="declaredNameEn" title="English Declared Name"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="declaredNameCn" title="Chinese Declared Name"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="isVirtual" title="Is Virtual"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="isVariant" title="Is Variant"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="virtualVariantMethodEnum" title="Virtual Variant Method"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="requirementMethodEnumId" title="Requirement Method"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="supplierPartyId" title="Supplier Party ID"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="priceUSD" title="USD Price"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="priceGBP" title="GBP Price"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="priceAUD" title="AUD Price"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="priceEUR" title="EUR Price"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="priceCAD" title="CAD Price"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="importedStatus" title="Imported"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        <field name="importedDescription" title="Message"  position="1" use-when="${groovy:String prev=(String)previousItem.get(&quot;productImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(productImportId)));}"><display/></field>
        
        <!--<sort-order>
            <sort-field name="productId"/>
        </sort-order>-->
    </form>
    
    <form name="editTemporaryProduct" type="single" target="updateProductImport" default-entity-name="ProductImport">
        <auto-fields-entity entity-name="ProductImport" map-name="productImport"/>
        <field name="productImportId"><display/></field>
        <field name="fileName"><display/></field>
        <field name="fileLineNumber"><display/></field>
        <field name="facilityId">
            <drop-down allow-empty="true">
                <entity-options entity-name="Facility" description="${description}"> <!-- [${productTypeId}] -->
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="productTypeId">
            <drop-down no-current-selected-key="FINISHED_GOOD">
                <entity-options entity-name="ProductType" description="${description}"> <!-- [${productTypeId}] -->
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="isVirtual">
            <drop-down><option key="N" description="${uiLabelMap.CommonN}"/><option key="Y" description="${uiLabelMap.CommonY}"/></drop-down>
        </field>
        <field name="isVariant">
            <drop-down><option key="N" description="${uiLabelMap.CommonN}"/><option key="Y" description="${uiLabelMap.CommonY}"/></drop-down>
        </field>
        <field name="virtualVariantMethodEnum">
            <drop-down allow-empty="true">
                <entity-options entity-name="Enumeration" key-field-name="enumId" description="${description}">
                    <entity-constraint name="enumTypeId" value="PROD_VVMETHOD"/>
                    <entity-order-by field-name="sequenceId"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="requirementMethodEnumId">
            <drop-down allow-empty="true">
                <entity-options entity-name="Enumeration" key-field-name="enumId" description="${description}">
                    <entity-constraint name="enumTypeId" value="PROD_REQ_METHOD"/>
                    <entity-order-by field-name="sequenceId"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="importedStatus"><display/></field>
        <field name="importedDescription"><display/></field>
        <field name="submit"><submit></submit></field>
    </form>
    
