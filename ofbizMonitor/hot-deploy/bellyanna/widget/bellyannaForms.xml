    <form name="productListingStatus" type="single" target="productListingStatus" header-row-style="header-row" default-table-style="basic-table">
        <field name="listingStatus" title="Listing Status">
            <drop-down no-current-selected-key="Listed">
                <option key="LISTED" description="Listed"/>
                <option key="NOT_LISTED" description="Not Listed"/>
            </drop-down>
        </field>
        <field name="productStoreId" title="${uiLabelMap.ProductStoreId}">
            <drop-down no-current-selected-key="All">
                <option key="All" description="All"/>
                <option key="" description="=========================="/>
                <entity-options entity-name="ProductStore" description="${subtitle} (${productStoreId})">
                    <entity-constraint name="primaryStoreGroupId" value="EBAY"/>
                    <entity-order-by field-name="subtitle"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="productId"><text size="30" maxlength="50"/></field>
        <field name="productStatus" title="Product Status">
            <drop-down no-current-selected-key="All">
                <option key="All" description="All"/>
                <option key="ACTIVE" description="Active"/>
                <option key="DISCONTINUED" description="Discontinued"/>
            </drop-down>
        </field>
        <field name="inventoryStatus" title="Inventory Status">
            <drop-down no-current-selected-key="All">
                <option key="All" description="All"/>
                <option key="IN_STOCK" description="In Stock"/>
                <option key="OUT_OF_STOCK" description="Out of Stock"/>
            </drop-down>
        </field>
        <field name="quantity" title="Quantity Less than or Equal to">
            <text size="5" maxlength="10" default-value="2"/>
        </field>
        <field name="submitButton" title="Search" widget-style="smallSubmit"><submit button-type="button"/></field>
    </form>
    
    <form name="selectImportForm" type="single" header-row-style="header-row" default-table-style="basic-table">
