    <service name="updateEbayActiveListing" auth="true" engine="java" use-transaction="false" transaction-timeout="720000"
        location="com.bellyanna.ebay.eBayTradingAPI" invoke="updateEbayActiveListing">
        <description>Update eBay Active Listing via eBay Trading API GetSellerList</description>
        <attribute name="productStoreId" type="String" mode="INOUT" optional="true"/>
        <attribute name="pageNumber" type="String" mode="IN" optional="true"/>
        <attribute name="removeExisting" type="String" mode="IN" optional="true"/>
        <attribute name="productStoreGroup" type="String" mode="INOUT" optional="true"/>
        <attribute name="continuousMode" type="String" mode="IN" optional="true"/>
    </service>
    
    <service name="TradingApiVerifyUpdateListing" auth="true" engine="java" use-transaction="false"
        location="com.bellyanna.ebay.eBayTradingAPI" invoke="verifyUpdateListing">
