<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>bellyanna Services for eBay related</description>
    <vendor></vendor>
    <version>1.0</version>
    
    <!-- common - START-->
    <service name="discontinueAdjustEbayQuantity" auth="true" engine="java" transaction-timeout="360000"
        location="com.bellyanna.ebay.common" invoke="discontinueAdjustEbayQuantity">
        <description>Adjust eBay Quantity to 0 for discontinued product</description>
        <attribute name="fileName" type="String" mode="IN" optional="true"/>
        <attribute name="productUpdateId" type="String" mode="IN" optional="true"/>
    </service>
    <service name="checkDiscontinuedListingOnEbay" auth="true" engine="java" transaction-timeout="720000"
        location="com.bellyanna.ebay.common" invoke="checkDiscontinuedListingOnEbay">
        <description>Check Listing for Discontinued Products</description>
        <attribute name="productStoreId" type="String" mode="IN" optional="true"/>
    </service>
    <!-- common - END -->
    
