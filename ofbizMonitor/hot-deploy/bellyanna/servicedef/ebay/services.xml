        <attribute name="productStoreGroup" type="String" mode="IN" optional="true"/>
        <attribute name="productStoreId" type="String" mode="IN" optional="true"/>
        <!-- <attribute name="productStoreId" type="String" mode="OUT" optional="true"/> -->
    </service>

    <!-- eBayTradingAPI - get Seller List -->
    <service name="TradingApiGetSellerList" auth="true" engine="java" use-transaction="false"
