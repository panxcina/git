    <form name="importMultiOrders" type="single" target="importMultiOrders">
        <field name="productStoreId"><hidden value="${productStoreId}"/></field>
        <field name="fileName"><hidden value="${fileName}"/></field>
        <field name="submit" title="Import All Orders to OFBiz"><submit></submit></field>
    </form>
    <form name="editTemporaryOrder" type="single" target="updateOrderImport" default-entity-name="OrderImport">
        <auto-fields-entity entity-name="OrderImport" map-name="orderImport"/>
        <field name="orderImportId"><display/></field>
        <field name="fileName"><display/></field>
        <field name="fileLineNumber"><display/></field>
        <field name="countryGeoId">
            <drop-down>
                <entity-options key-field-name="geoId" description="${geoName} - [${geoId}]" entity-name="Geo">
                    <entity-constraint name="geoTypeId" value="COUNTRY"/>
                    <entity-order-by field-name="geoName"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="currencyUom">
            <drop-down>
                <entity-options key-field-name="uomId" description="${description} - [${uomId}]" entity-name="Uom">
                    <entity-constraint name="uomTypeId" value="CURRENCY_MEASURE"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="paymentMethodTypeId">
            <drop-down>
                <entity-options key-field-name="paymentMethodTypeId" description="${description} - [${paymentMethodTypeId}]" entity-name="PaymentMethodType">
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="productStoreShipMethId">
            <drop-down>
                <entity-options description="${productStoreShipMethId} - ${description} - ${shipmentMethodTypeId} - [${partyId}]" key-field-name="productStoreShipMethId" entity-name="ProductStoreShipmentMethView">
                    <entity-constraint name="productStoreId" value="${productStoreId}"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="importedStatus"><display/></field>
        <field name="importedDescription"><display/></field>
        <field name="submit"><submit></submit></field>
    </form>
    <form name="editTemporaryOrderItem" type="list" target="updateOrderItemImport" default-entity-name="OrderItemImport" list-name="orderItemImports"
        use-row-submit="true" header-row-style="header-row-2" default-table-style="basic-table hover-bar" odd-row-style="alternate-row" view-size="50">
        <auto-fields-entity entity-name="OrderItemImport"/>
        <field name="productStoreId"><hidden></hidden></field>
        <field name="fileName"><hidden></hidden></field>
        <field name="productId"><lookup target-form-name="LookupProduct"/></field>
        <field name="orderImportId"><display/></field>
        <field name="orderItemImportSeqId"><display/></field>
        <field name="fileLineNumber"><display/></field>
        <field name="submit"><submit></submit></field>
    </form>
    <form name="shippingMethodList" type="list" target="" title="" list-name="enumerations"
        odd-row-style="alternate-row" default-table-style="basic-table">
        <actions>
            <entity-and entity-name="Enumeration" list="enumerations">
                <field-map field-name="enumTypeId" value="PROSTORE_EXSHIP"/>
                <field-map field-name="enumCode" from-field="productStoreId"/>
            </entity-and>
        </actions>
        <row-actions>
            <entity-one entity-name="ProductStoreShipmentMeth" value-field="productStoreShipmentMeth">
                <field-map field-name="productStoreShipMethId" value="${enumId}"/>
            </entity-one>
            <entity-one entity-name="ShipmentMethodType" value-field="shipmentMethodType">
                <field-map field-name="shipmentMethodTypeId" from-field="productStoreShipmentMeth.shipmentMethodTypeId"/>
            </entity-one>
        </row-actions>
        <field name="productStoreId"><display description="${productStoreShipmentMeth.productStoreId}"/></field>
        <field name="shipmentMethodTypeId" title="Ofbiz Shipping Method"><display description="${shipmentMethodType.description} - [${productStoreShipmentMeth.shipmentMethodTypeId}]"/></field>
        <field name="description" title="External Shipping Method"><display/></field>
    </form>
    
    <form name="shippingMethodEdit" type="multi" target="updateProductStoreShippingEnum" title="" list-name="enumerations"
        odd-row-style="alternate-row" default-table-style="basic-table" view-size="50">
        <actions>
            <entity-and entity-name="EnumerationType" list="enumerationTypes">
                <field-map field-name="description" from-field="productStoreId"/>
                <field-map field-name="parentTypeId" value="PROSTORE_EXSHIP"/>
            </entity-and>
            <set field="enumParentType" from-field="enumerationTypes[0].enumTypeId"/>
            <entity-and entity-name="Enumeration" list="enumerations">
                <field-map field-name="enumTypeId" from-field="enumParentType"/>
            </entity-and>
        </actions>
        <field name="enumId"><hidden value="${enumId}"/></field>
        <field name="productStoreId" title="ProductStoreId">
            <display-entity entity-name="ProductStore" key-field-name="productStoreId" description="${storeName}">
                <sub-hyperlink target="/catalog/control/EditProductStore" description="[${productStoreId}]" target-type="inter-app">
                    <parameter param-name="productStoreId"/>
                </sub-hyperlink>
            </display-entity>
        </field>
        <field name="enumCode" title="Ofbiz ProductStore Shipping Method">
            <drop-down>
                <entity-options description="${productStoreShipMethId} - ${description} - ${shipmentMethodTypeId} - [${partyId}]" key-field-name="productStoreShipMethId" entity-name="ProductStoreShipmentMethView">
                    <entity-constraint name="productStoreId" value="${productStoreId}"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="enumTypeId" ><hidden value="${enumParentType}"/></field>
        <field name="description" title="External Shipping Method"><text size="50" default-value="${description}"></text></field>
        
        <field name="delete" widget-style="buttontext">
            <hyperlink target="deleteEnumeration" description="Delete" also-hidden="false" request-confirmation="true" confirmation-message="Are you sure to delete this record">
                <parameter param-name="enumId"/>
            </hyperlink>
        </field>
        <field name="submit" title="Update"><submit></submit></field>
        <sort-order>
            <sort-field name="productStoreId"/>
            <sort-field name="enumCode"/>
            <sort-field name="description"/>
        </sort-order>
    </form>
    <form name="addNewShippingMethod" type="single" target="createProductStoreShippingEnum" >
        <actions>
            <entity-and entity-name="EnumerationType" list="enumerationTypes">
                <field-map field-name="description" from-field="productStoreId"/>
                <field-map field-name="parentTypeId" value="PROSTORE_EXSHIP"/>
            </entity-and>
            <set field="enumParentType" from-field="enumerationTypes[0].enumTypeId"/>
        </actions>
        <field name="productStoreId"><hidden/></field>
        <field name="enumTypeId"><hidden value="${enumParentType}"/></field>
        <field name="enumCode" title="Ofbiz ProductStore Shipping Method">
            <drop-down>
                <entity-options description="${productStoreShipMethId} - ${description} - ${shipmentMethodTypeId} - [${partyId}]" key-field-name="productStoreShipMethId" entity-name="ProductStoreShipmentMethView">
                    <entity-constraint name="productStoreId" value="${productStoreId}"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="description" title="External Shipping Service"><text size="100"/></field>
         <field name="submit" title="Create"><submit></submit></field>
    </form>
    <form name="addNewPaymentMethod" type="single" target="addNewPaymentMethodMapping" >
        <field name="enumTypeId"><hidden value="PAYMENT_MAP"/></field>
        <field name="paymentMethodTypeId" title="Ofbiz Payment Method Type Id" required-field="true">
            <drop-down>
                <entity-options description="${description}" entity-name="PaymentMethodType" key-field-name="paymentMethodTypeId">
                </entity-options>
            </drop-down>
        </field>
        <field name="externalPaymentMethod" title="External Shipping Service" required-field="true"><text size="100"/></field>
        <field name="description" required-field="true"><textarea/></field>
         <field name="submit" title="Create"><submit></submit></field>
    </form>
    <form name="paymentMethodMappingEdit" type="multi" target="updateEnumeration" title="" list-name="enumerations"
        odd-row-style="alternate-row" default-table-style="basic-table" view-size="50">
        <actions>
            <entity-and entity-name="Enumeration" list="enumerations">
                <field-map field-name="enumTypeId" value="PAYMENT_MAP"/>
            </entity-and>
        </actions>
        <auto-fields-service service-name="updateEnumeration"/>
        
        <field name="enumId" title="Ofbiz Payment Method"><display description="${enumId}"/></field>
        <field name="enumTypeId" ><hidden value="PAYMENT_MAP"/></field>
        <field name="sequenceId"><hidden/></field>
        <field name="description" title="Description"><text/></field>
        <field name="enumCode" title="External Shipping Method"><text size="60" default-value="${enumCode}"></text></field>
        
        <field name="delete" widget-style="buttontext">
            <hyperlink target="deleteEnumeration" description="Delete" also-hidden="false">
                <parameter param-name="enumId"/>
            </hyperlink>
        </field>
        <field name="submit" title="Update"><submit></submit></field>
    </form>

    <form name="importInternalOrder" type="upload" target="doUploadInternalOrderCsvFile">
        <field name="uploadedFile"><file size="50"/></field>
