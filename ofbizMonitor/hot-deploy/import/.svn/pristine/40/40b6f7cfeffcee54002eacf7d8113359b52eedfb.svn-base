        <field name="address1" title="Address1"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="address2" title="Address2"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="city" title="City"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="stateProvinceGeoId" title="State Province Geo Id"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="postalCode" title="Postal Code"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="countryGeoId" title="Country Geo Id"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="currencyUom" title="Currency Uom"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="remainingSubTotal" title="Remaining Sub Total"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="shippingAmount" title="Shipping Amount"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="taxAmount" title="Tax Amount"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="taxAdjustmentAmount" title="Tax Adjustment Amount"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="warrantyAmount" title="Warranty Amount"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="grandTotalAmount" title="Grand Total Amount"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="paymentMethodTypeId" title="Payment Method Type Id"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="createDate" title="Create Date"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="checkoutDate" title="Checkout Date"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="paidOnDate" title="Paid On Date"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="shippedOnDate" title="Shipped On Date"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="noteInfo" title="Note Info"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
