<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">
    <form name="importOrder" type="upload" target="doUploadOrderCsvFile">
        <field name="productStoreId" title="ProductStoreId">
            <display-entity entity-name="ProductStore" key-field-name="productStoreId" description="${storeName} [${productStoreId}]">
                <sub-hyperlink target="importOrder" description="[change]" target-type="inter-app"/>
            </display-entity>
        </field>
        <field name="uploadedFile"><file size="50"/></field>
        <field name="submit" title="Upload"><submit/></field>
    </form>
    
    <form name="listOrderImport" type="list" list-name="listIt" odd-row-style="alternate-row" default-table-style="basic-table" view-size="10" paginate-target="importOrder"> 
        <actions>
            <script location="component://import/webapp/import/WEB-INF/actions/ListFilesInDirectory.groovy"/>
        </actions>
        <field name="uploadDate" title="Uploaded Date"><display/></field>
        <field name="name"><display/></field>
        <field name="path"><display/></field>
        <field name="allRecords"><display/></field>
        <field name="importedRecords" title="Imported"><display/></field>
        <field name="unImportRecords" title="Unimport"><display/></field>
        <field name="viewOrderImport" title="" widget-style="buttontext">
            <hyperlink target="viewOrderImport" description="View Orders" also-hidden="false" link-type="hidden-form">
                <parameter param-name="productStoreId"/>
                <parameter param-name="fileName" from-field="path"/>
                <parameter param-name="name"/>
            </hyperlink>
        </field>
        <field name="deleteFileOrderImport" title="" widget-style="buttontext">
            <hyperlink target="deleteFileOrderImport" description="Clear Temporary Data and File" also-hidden="false" confirmation-message="Are you sure to delete this record?">
                <parameter param-name="productStoreId"/>
                <parameter param-name="fileName" from-field="path"/>
            </hyperlink>
        </field>
    </form>

    <form name="selectProductStore" type="single" header-row-style="header-row" default-table-style="basic-table">
        <field name="productStoreId" event="onChange" action="javascript:
            document.selectProductStore.action='importOrder'; 
            document.selectProductStore.submit();">
            <drop-down allow-empty="true">
                <entity-options entity-name="ProductStore" description="${storeName} - [${productStoreId}]" key-field-name="productStoreId"/>
            </drop-down>
        </field>
    </form>

    <form name="viewOrderImport" type="list" list-name="orderImportOrderItemImports" use-row-submit="true" target="importMultipleOrders" 
        header-row-style="header-row-2" default-table-style="basic-table hover-bar" odd-row-style="alternate-row" view-size="50" paginate-target="viewOrderImport">
        <field name="fileName"><hidden value="${fileName}"/></field>
        <field name="createOrder" title=" " widget-style="buttontext" position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);
            if (prev == null) prev = &quot;allowFirst&quot;;
            return new Boolean(prev!=null&amp;&amp;!(prev.equals(orderImportId))&amp;&amp;importedStatus.equals(&quot;N&quot;));}">
            <hyperlink target="importNewOrderForImportAndMessage" description="Import" also-hidden="false">
                <parameter param-name="productStoreId" from-field="productStoreId"/>
                <parameter param-name="fileName" from-field="fileName"></parameter>
                <parameter param-name="orderImportId"/>
            </hyperlink>
        </field>
        <field name="editOrder" title=" " widget-style="buttontext" position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);
            if (prev == null) prev = &quot;allowFirst&quot;;
            return new Boolean(prev!=null&amp;&amp;!(prev.equals(orderImportId))&amp;&amp;importedStatus.equals(&quot;N&quot;));}">
            <hyperlink target="editTemporaryOrder" description="Edit" also-hidden="false" link-type="hidden-form">
                <parameter param-name="orderImportId"/>
                <parameter param-name="productStoreId"/>
                <parameter param-name="fileName"/>
                <parameter param-name="imported"/>
            </hyperlink>
        </field>
        <!-- OrderImport entity fields -->
        <field name="orderImportId" title="Order Import Id" position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="fileLineNumber" title="File Line Number" position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="orderId" title="Order Id"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId))&amp;&amp;importedStatus.equals(&quot;N&quot;));}"><display/></field>
        <field name="orderId" title="Order Id"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId))&amp;&amp;importedStatus.equals(&quot;Y&quot;));}" widget-style="buttontext">
            <hyperlink target="/ordermgr/control/orderview" description="${orderId}" target-type="inter-app" target-window="blank">
                <parameter param-name="orderId"/>
            </hyperlink>
        </field>
        <field name="productStoreId" title="Product Store Id"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="userId" title="User Id"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="firstName" title="First Name" position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="middleName" title="Middle Name"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="lastName" title="Last Name"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="toName" title="To Name"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="countryCode" title="Country Code"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
        <field name="areaCode" title="Area Code"  position="1" use-when="${bsh:String prev=(String)previousItem.get(&quot;orderImportId&quot;);return new Boolean(!(prev!=null&amp;&amp;prev.equals(orderImportId)));}"><display/></field>
